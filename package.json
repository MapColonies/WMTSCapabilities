{
  "name": "@map-colonies/wmts-layer-leaflet",
  "version": "1.0.0",
  "description": "This is an NPM package that allows creating leaflet WMTS layers by capabilities",
  "main": "./src/main.js",
  "scripts": {
    "test:unit": "jest --config=./tests/configurations/unit/jest.config.js",
    "test:integration": "jest --config=./tests/configurations/integration/jest.config.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "standard-version",
    "test": "npm run test:unit && npm run test:integration",
    "prebuild": "npm run clean",
    "build": "",
    "start": "node ./src/main.js",
    "clean": "",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MapColonies/wmts-layer-leaflet.git"
  },
  "author": "MapColonies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MapColonies/wmts-layer-leaflet/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "https://github.com/MapColonies/wmts-layer-leaflet#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@map-colonies/eslint-config": "^3.1.3",
    "@map-colonies/prettier-config": "0.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^14.14.12",
    "commitlint": "^17.4.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.36.0",
    "husky": "^4.3.8",
    "jest": "^29.5.0",
    "jest-create-mock-instance": "^2.0.0",
    "jest-html-reporters": "^3.1.4",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "standard-version": "^9.5.0",
    "typedoc": "^0.24.8"
  },
  "dependencies": {
    "@map-colonies/wmts-capabilities-parser": "file:../WMTSCapabilitiesParser/map-colonies-wmts-capabilities-parser-1.0.0.tgz"
  },
  "bundleDependencies": [
    "@map-colonies/wmts-capabilities-parser"
  ]
}
